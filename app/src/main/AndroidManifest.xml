<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.carloshoil.taxiliteconductor">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TaxiLiteConductor">
        <activity android:name=".ActivityLoginPhone"></activity>
        <activity android:name=".RegistroCuentaActivity"></activity>
        <activity android:name=".SplashActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistroDatosActivity" />
        <activity android:name=".ActivityDatos" />
        <activity android:name=".PrincipalActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>